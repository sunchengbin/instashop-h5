<div class="address-wraper">
<nav class="header-nav clearfix">
    <i class="icon iconfont j_go_back icon-back-font"></i>
    Konfirmasi Pembayaran
</nav>
<section class="form-box">
    <div class="form-li clearfix">
        <div class="b-bottom g-color b-right form-titie">Nama Pengirim di Rekening Bank :</div>
        <div class="b-bottom"><input type="text" value="{{data.evidence.account_name}}" maxlength="50" placeholder="" class="j_bank"/></div>
    </div>
    <div class="form-li clearfix">
        <div class="b-bottom g-color b-right">Transfer dari :</div>
        <div class="b-bottom"><input type="text" value="{{data.evidence.trans_from}}" placeholder="" class="j_from"/></div>
    </div>
    <div class="form-li clearfix">
        <div class="b-bottom g-color b-right">Transfer ke :</div>
        <div class="b-bottom"><input type="text" value="{{data.evidence.trans_to}}"  placeholder="" class="j_to"/></div>
    </div>
    <div class="form-li clearfix">
        <div class="b-bottom g-color b-right">Jumlah :</div>
        <div class="b-bottom"><input type="tel" value="{{transpriceval data.evidence.price}}" maxlength="16" placeholder="" class="j_price"/></div>
    </div>
    <div class="form-li clearfix">
        <div class="b-bottom g-color b-right">Tanggal Transfer :</div>
        <div class="b-bottom"><input type="date" value="{{data.evidence.date}}" placeholder="" class="j_time"/></div>
    </div>
</section>
<button class="submit-btn btn confirm-btn j_sub_btn">Ajukan</button>
<section class="address-list-box j_address_list_box">
    <nav class="header-nav address-header-nav j_address_header clearfix">
        <i class="icon iconfont j_go_address icon-back-font"></i>
    </nav>
    <section class="address-list j_address_list">
        <div class="list-box j_list_box">

        </div>
    </section>
</section>
</div>