<section class="address-form-box">
    <nav class="header-nav clearfix">
        <button class="fr j_save_address">{{lang.H5_DONE}}</button>
        <i class="icon iconfont j_go_back">&#xe62a;</i>
        {{lang.H5_SHIPPING_ADDRESS}}
    </nav>
    <section class="address-wraper">
        <div class="user-info">
            <div class="user-name info-box">
                <input class="j_name" type="text" value="{{name}}" placeholder="{{lang.H5_USER_NAME_PH}}"/>
            </div>
            <div class="user-tel info-box">
                <input class="j_tel" type="tel" value="{{telephone}}" placeholder="{{lang.H5_USER_TELE_PH}}"/>
            </div>
        </div>
        <div class="user-address j_user_address">
            <div class="info-box act clearfix" data-name="province">
                <i class="icon iconfont fr">&#xe63e;</i>
                <p class="j_province">{{#if address.province}}{{address.province}}{{else}}{{lang.H5_PROVINCE}}{{/if}}</p>
            </div>
            <div class="info-box {{#if address.city}}act{{/if}} clearfix" data-name="city">
                <i class="icon iconfont fr">&#xe63e;</i>
                <p class="j_city">{{#if address.city}}{{address.city}}{{else}}{{lang.H5_CITY}}{{/if}}</p>
            </div>
            <div class="info-box {{#if address.country}}act{{/if}} clearfix" data-name="country">
                <i class="icon iconfont fr">&#xe63e;</i>
                <p class="j_country">{{#if address.country}}{{address.country}}{{else}}{{lang.H5_DISTRICT}}{{/if}}</p>
            </div>
            <div class="info-box-address">
                <textarea class="j_street" placeholder="{{lang.H5_DETAIL_ADDRESS}}">{{#if address.street}}{{address.street}}{{/if}}</textarea>
            </div>
        </div>
    </section>
</section>
