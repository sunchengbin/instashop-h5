<section class="logistics-plug j_logistics_plug">
    <div class="logistics-plug-top clearfix">
        <i class="icon iconfont j_close_logistics_btn fr icon-cancel-font"></i>
        <p class="title"></p>
    </div>
    <div class="logistics-plug-info">
        <ul class="logistics-list">
            {{#if data.line}}
                <li class="j_logistics_li b-bottom">
                    <a href="{{data.line}}">{{lang.H5_LINE_CONTACT_ME}}</a>
                </li>
            {{/if}}
            {{#if data.tel}}
                <li class="j_logistics_li b-bottom">
                    <a href="tel:{{data.tel}}">{{lang.H5_CALL_TELEPHONE}}</a>
                </li>
                <li class="j_logistics_li">
                    <a href="sms:{{data.tel}}">{{lang.H5_SEND_SMS}}</a>
                </li>
            {{/if}}
        </ul>
    </div>
</section>
<section class="logistics_plug_cover j_logistics_plug_cover"></section>